package com.day7.stringsPrograms;

public class RevereseTheString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String s="hello world";
		String res = null;
		
		for (int i = s.length(); i >=0; i--) {
			res=res+s.charAt(i);
		}
		System.out.println(res);
	}

}
